//Ingresar 2 números y determinar cuál es el mayor de los dos.
Algoritmo CONDICIONALES_02
	Definir numeroUno Como Entero;
	Definir numeroDos Como Entero;
	
	Mostrar "Ingresar número A ", Sin Saltar;
	Leer numeroUno;
	Mostrar "Ingresar número B ", Sin Saltar;
	Leer numeroDos;
	
	Si numeroUno<numeroDos Entonces;
		Mostrar "Numero B es mayor que número A";
	SiNo;
		Mostrar "Número A es mayor que número B";
	FinSi;
	
FinAlgoritmo
